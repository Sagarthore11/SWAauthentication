{
  "routes": [
    {
      "route": "/",
      "allowedRoles": ["anonymous", "authenticated"]
    }
  ],
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "registration": {
          "openIdIssuer": "https://login.microsoftonline.com/<your-tenant-id>/v2.0",
          "clientIdSettingName": "AAD_CLIENT_ID"
        }
      }
    }
  }
}
